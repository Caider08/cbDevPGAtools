@{
}

<h1>@ViewBag.Game</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach (Golfer golfer in ViewBag.Golfers)
    {
    <tr>
        <td>@golfer.ID</td>
        <td>@golfer.Name</td>
        <td>@golfer.Salary</td>
    </tr>
    }
</table>


